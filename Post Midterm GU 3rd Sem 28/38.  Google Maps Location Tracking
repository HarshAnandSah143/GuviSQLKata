Q1)
SELECT DISTINCT T1.location_name
FROM locations AS T1
INNER JOIN user_checkins AS T2
  ON T1.id = T2.location_id
WHERE T1.latitude>12.940000;

Q2)
select user_id 
from user_checkins u 
join locations l 
on u.location_id = l.id 
where l.location_name = 'Bookstore';

Q3)
SELECT
    L.location_name
FROM
    locations L
LEFT JOIN
    user_checkins C ON L.id = C.location_id
WHERE
    C.location_id IS NULL;

Q4)
SELECT l.location_name, COUNT(uc.user_id)
FROM locations l
JOIN user_checkins uc 
ON l.id = uc.location_id
GROUP BY l.location_name;