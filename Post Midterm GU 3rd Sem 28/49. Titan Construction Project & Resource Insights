Q1)
SELECT cp.project_name, cp.budget, pe.engineer_name, pe.role
FROM construction_projects cp
INNER JOIN project_engineers pe
ON cp.project_id = pe.project_id
WHERE cp.budget > 100000000;

Q2)
SELECT ROUND(AVG(budget), 2) 
AS average_budget
FROM construction_projects;

Q3)
BEGIN TRANSACTION;
UPDATE construction_projects
SET budget = 250000000
WHERE project_id = 1;

ROLLBACK;
