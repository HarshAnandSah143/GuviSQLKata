Q25)
SELECT first_name,salary,hire_date 
FROM employees 
WHERE hire_date LIKE '2001%' 
ORDER BY salary DESC;

Q26)
SELECT first_name, last_name, email, job_id, salary
FROM employees
WHERE job_id = 'IT_PROG';

Q27)
